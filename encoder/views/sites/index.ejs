<html>
  <head>
    <title>Encoder | Caloocan 2025 Elections Quick Tracking System</title>
    <link rel="stylesheet" href="stylesheets/encoder.css" />
    <link rel="stylesheet" href="stylesheets/login.css" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4Q6Gf2aSP4eDXB8Miphtr37CMZZQ5oXLH2yaXMJ2w8e2ZtHTl7GptT4jmndRuHDT"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <%- include('../partials/header'); %>
    <div class="encoderBody">
      <div class="precinctsBody">
        <h2>Precincts Assigned</h2>
        <div class="precinctsList list-group">
          <% precincts.forEach(function(precinct) { %>
          <button class="clusteredPrecinct list-group-item">
            <%= precinct %>
          </button>

          <% }) %>
        </div>
      </div>
      <div class="container tallies">
        <h3 class="text-center tallyHeader">Precinct 0005A, District 1</h3>
        <div class="row">
          <div class="col">
            <%- include('../partials/tally-card', { header: 'Mayor', candidates:
            mayors }) %>
          </div>
          <div class="col">
            <%- include('../partials/tally-card', { header: 'Vice-Mayor',
            candidates: viceMayors }) %>
          </div>
        </div>
        <div class="row">
          <div class="col">
            <%- include('../partials/tally-card', { header: 'Congressman',
            candidates: congressmen }) %>
          </div>
          <div class="col">
            <%- include('../partials/tally-card', { header: 'Councilors',
            candidates: congressmen }) %>
          </div>
        </div>
      </div>
    </div>
    <%- include('../partials/footer'); %>
  </body>
</html>
